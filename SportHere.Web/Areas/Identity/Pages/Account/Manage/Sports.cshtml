@page "{currentpage:int?}"
@model SportHere.Web.Areas.Identity.Pages.Account.Manage.SportsModel
@{
}

<h1 class="text-center mb-3">Változtassa meg sportjait!</h1>

@using (Html.BeginForm())
{
    <div class="row">
        @Html.EditorFor(m => m.SportokPagedList, additionalViewData: new { handler = "check" })
    </div>

    @Html.DisplayFor(m => m.Pagination, additionalViewData: new { currentPage = Model.CurrentPage })
}